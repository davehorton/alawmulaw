{
  "name": "alawmulaw",
  "version": "1.1.0",
  "description": "JavaScript A-Law and mu-Law codecs.",
  "homepage": "https://github.com/rochars/alawmulaw",
  "author": "Rafael da Silva Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "keywords": [
    "wav",
    "A-Law",
    "mu-Law",
    "audio",
    "codec",
    "encode",
    "decode",
    "compander",
    "companding",
    "PCM",
    "file",
    "streaming"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/alawmulaw.git"
  },
  "scripts": {
    "lint": "jshint index.js",
    "test": "nyc ./node_modules/mocha/bin/_mocha --recursive",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "bundle": "webpack",
    "compile": "google-closure-compiler-js dist/alawmulaw.js > dist/alawmulaw-min.js",
    "doc": "./node_modules/.bin/jsdoc index.js src -d docs -r README.md -t node_modules/docdash",
    "qa": "npm run lint && npm run test",
    "pack": "npm run bundle && npm run compile && npm run doc",
    "build": "npm run qa && npm run pack"
  },
  "devDependencies": {
    "codecov": "^3.0.0",
    "docdash": "^0.4.0",
    "google-closure-compiler-js": "^20170910.0.1",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "string-replace-loader": "^1.3.0",
    "webpack": "^3.8.1"
  },
  "main": "index.js",
  "dependencies": {}
}
